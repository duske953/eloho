    <div class="contact inactive">
      <form class="contact__form" action="/" method="post">
        <p class="contact__heading">Get in touch</p>
        <ion-icon class="contact__icon-close" name="close-outline"></ion-icon>
        <div class="contact__box">
          <label class="contact__label" for="name">Name *</label>
          <input
            class="contact__input"
            placeholder="Your name"
            id="name"
            name="name"
            type="text"
          />
          <span class="contact__err contact__err--inactive"
            >Your name is required</span
          >
        </div>

        <div class="contact__box">
          <label class="contact__label" for="email">Email *</label>
          <input
            class="contact__input"
            placeholder="Your email"
            id="email"
            name="email"
            type="email"
          />
          <span class="contact__err contact__err--inactive"
            >Your email is required</span
          >
        </div>

        <div class="contact__box">
          <label for="message" class="contact__label" for="name"
            >Message *</label
          >
          <textarea
            style="resize: none"
            class="contact__text-area"
            placeholder="what are you thinking..."
            name="message"
            id="message"
            cols="30"
            rows="2"
          ></textarea>
          <span class="contact__err contact__err--inactive"
            >Message is required</span
          >
        </div>
        <button type="submit" class="contact__btn-submit">Send message</button>
      </form>
    </div>

    <div class="overlay inactive"></div>

    <div class="message-box inactive-message">
      <p class="message">
        Message received! I'll get back to you as soon as possible
      </p>
    </div>
    <div class="lds-roller spinner-border inactive">
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
